[{
    "settings": ["master"],
    
    "specs": {
        "htmlframe": {
            "type": "ShakerHTMLFrameMojit",
            "config": {
                "deploy": true,
                "title": "Shaker Demo",
                "child": {
                    "type": "master"
                }
            }
        }
    }
},
{
    "settings": ["environment:test"],

    "shaker": {
        "compile": true,
        "push": {
            "type": "local",
            "config": {
                "root": "assets/compiled"
            }
        }
    }
},
{
    "settings": ["environment:prod"],

    "shaker": {
        "compile": true,
        "push": {
            "type": "mobstor",
            "config": {
                "parallel": 1,
                "delay": 200,
                "root": "assets/compiled",
                "connection": {
                    "host": "playground.yahoofs.com",
                    "proxy": {
                        "host": "yca-proxy.corp.yahoo.com",
                        "port": 3128
                    }
                }
            }
        }
    }
}
]
